<?xml version="1.0" encoding="utf-8"?>
<!--
  安全配置文件模板

  使用方法：
  1. 复制此文件并重命名为 LB_FATE.Mobile.csproj.user
  2. 填入实际的密码（不要提交到版本控制）
  3. 确保 .gitignore 已忽略 *.user 文件
-->
<Project>
  <PropertyGroup>
    <!-- Android 签名密钥密码 -->
    <!-- 用于 Release 构建时签名 APK -->
    <AndroidSigningPassword>YOUR_KEYSTORE_PASSWORD_HERE</AndroidSigningPassword>
  </PropertyGroup>

  <!-- 可选：Windows 证书密码（如果使用带密码的 PFX） -->
  <!--
  <PropertyGroup>
    <PackageCertificatePassword>YOUR_CERTIFICATE_PASSWORD_HERE</PackageCertificatePassword>
  </PropertyGroup>
  -->
</Project>
